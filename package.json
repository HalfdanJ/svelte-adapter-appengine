{
  "name": "svelte-adapter-appengine",
  "version": "0.10.3",
  "description": "Adapter for Svelte that integrates with Google Cloud Appengine ",
  "scripts": {
    "fix": "xo --fix",
    "test": "xo",
    "test:integration": "tests/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/halfdanj/svelte-adapter-appengine.git"
  },
  "homepage": "https://github.com/halfdanj/svelte-adapter-appengine#readme",
  "bugs": {
    "url": "https://github.com/halfdanj/svelte-adapter-appengine/issues"
  },
  "author": "Jonas Jongejan <jonas@halfdanj.dk>",
  "license": "apache-2.0",
  "keywords": [
    "svelte",
    "sveltekit",
    "sveltekit-adapter",
    "appengine"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "index.js",
  "files": [
    "files"
  ],
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "start-server-and-test": "^2.0.0",
    "xo": "^0.54.1"
  },
  "dependencies": {
    "esbuild": "^0.17.3",
    "polka": "^1.0.0-next.22",
    "sirv": "^2.0.2",
    "yaml": "^2.2.1"
  },
  "xo": {
    "space": true,
    "rules": {
      "no-useless-call": [
        "off"
      ],
      "node/prefer-global/process": [
        "off"
      ],
      "unicorn/prefer-top-level-await": [
        "off"
      ],
      "n/file-extension-in-import": [
        "off"
      ],
      "xo/import-order": [
        "off"
      ]
    }
  }
}
